{
  "name": "openedtex-frontend",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "build:analyze": "vite build --mode analyze",
    "build:netlify": "NODE_ENV=production VITE_APP_ENV=production vite build --mode production",
    "build:netlify:preview": "NODE_ENV=production VITE_APP_ENV=preview vite build --mode production",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "type-check": "tsc --noEmit",
    "optimize": "npm run lint:fix && npm run build:netlify",
    "check-optimizations": "node optimize-build.js",
    "serve:optimized": "npm run build:netlify && npm run preview",
    "netlify:build": "./netlify-build.sh",
    "netlify:deploy": "npm run build:netlify && netlify deploy --prod --dir=dist",
    "netlify:preview": "npm run build:netlify && netlify deploy --dir=dist",
    "netlify:serve": "npm run build:netlify && npm run preview:dist",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules",
    "reinstall": "npm run clean:all && npm install",
    "postinstall": "npm run type-check || echo 'Skipping type-check on install'"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@reduxjs/toolkit": "^1.9.0",
    "@uiw/react-md-editor": "^4.0.8",
    "@vitejs/plugin-react": "^4.7.0",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.2.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-chartjs-2": "^5.2.0",
    "react-confirm-alert": "^3.0.6",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.8.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "react-spinners": "^0.13.8",
    "react-syntax-highlighter": "^15.6.6",
    "socket.io-client": "^4.7.0",
    "vite": "^5.4.19",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.1.6",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  }
}
