# Production Environment Configuration for schools.shatzii.com
NODE_ENV=production
PORT=3000
NEXT_PUBLIC_SITE_URL=https://schools.shatzii.com

# Database Configuration (optional)
DATABASE_URL=postgresql://username:password@localhost:5432/schools_db

# API Keys (Replace with actual values)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Security
SESSION_SECRET=generate_a_secure_random_string_here

# Domain Configuration
ALLOWED_ORIGINS=https://schools.shatzii.com,https://www.schools.shatzii.com
REPLIT_DOMAINS=schools.shatzii.com

# Performance
NEXT_TELEMETRY_DISABLED=1
